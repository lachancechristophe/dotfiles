;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	girafe.loops.lan. root.loops.lan. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	girafe.loops.lan.
loops	IN	A	192.168.56.2
btendre IN	A	192.168.56.2
@	IN	AAAA	::1
