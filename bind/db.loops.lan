;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	girafe.loops.lan. root.loops.lan. (
			     16		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
			IN	NS	girafe.loops.lan.
girafe.loops.lan.	IN	A	192.168.56.2

loops.lan.		IN	A	192.168.56.2
