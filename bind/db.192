;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	girafe.loops.lan. root.loops.lan. (
			     11		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	IN	NS	girafe.loops.lan.
2	IN	PTR	loops.lan.
2	IN	PTR	barres.lan.
2	IN	PTR	phpmyadmin.lan.
